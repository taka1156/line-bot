{
  "name": "line_bot",
  "version": "1.0.0",
  "main": "./src/index.js",
  "scripts": {
    "build:dev": "NODE_ENV=dev webpack --config ./webpack/dev.config.js",
    "build:prod": "NODE_ENV=prod webpack --config ./webpack/prod.config.js",
