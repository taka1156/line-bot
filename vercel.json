{
  "version": 2,
  "env": {
    "CHANNEL_ACCESS_TOKEN": "@channel_access_token",
    "CHANNEL_SECRET": "@channel_secret",
    "QIITA_TOKEN": "@qiita_token"
  },
  "builds": [
    {
      "src": "dist/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "(.*)", "dest": "dist/server.js", "methods": ["GET", "POST"] }
  ]
}
